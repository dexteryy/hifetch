{
  "name": "hifetch",
  "version": "1.0.0",
  "description": "A minimal higher-level wrapper around Fetch API",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/hifetch/issues"
  },
  "repository": "https://github.com/dexteryy/hifetch",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "lint": "eslint src/**",
    "build:src": "babel src --out-dir build/dist --source-maps",
    "build:test": "babel tests --out-dir build/tests --source-maps",
    "build": "if [ -d build ]; then rm -r build; fi; npm run build:src && npm run build:test",
    "test:nobuild": "mocha build/tests/**/*.spec.js",
    "test": "npm run build && npm run test:nobuild"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm run build"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "chance": "^1.0.4",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.8.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-react": "^6.4.1",
    "form-data": "^2.1.1",
    "ghooks": "^1.3.2",
    "mocha": "^3.1.2",
    "nock": "^8.1.0"
  },
  "keywords": [
    "fetch"
  ]
}
