{
  "name": "hifetch",
  "version": "2.1.1",
  "description": "A minimal higher-level wrapper around Fetch API",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/hifetch/issues"
  },
  "homepage": "https://github.com/dexteryy/hifetch",
  "repository": "https://github.com/dexteryy/hifetch",
  "license": "MIT",
  "main": "build/dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "precommit": "lint-staged && npm run test",
    "update": "rm -rf ./node_modules && yarn",
    "upgrade": "rm -rf yarn.lock package-lock.json && npm run update",
    "build": "if [ -d build ]; then rm -r build; fi; npm run build:src && npm run build:test",
    "build:src": "babel src --out-dir build/dist --source-maps",
    "build:test": "babel tests --out-dir build/tests --source-maps",
    "test": "npm run build && npm run test:nobuild",
    "test:nobuild": "mocha build/tests/**/*.spec.js",
    "lint": "eslint --fix --color --ext .js",
    "lint:error": "npm run lint -- --quiet"
  },
  "engines": {
    "node": ">=6"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:error --"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-syntax-optional-chaining": "^7.0.0-alpha.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chance": "^1.0.11",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.7.2",
    "eslint-config-webcube": "^1.0.0-rc.5",
    "fast-async": "^6.3.0",
    "form-data": "^2.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "mocha": "^4.0.0",
    "nock": "^9.0.22"
  },
  "keywords": [
    "fetch",
    "xhr",
    "ajax",
    "promise",
    "error-handling",
    "isomorphic"
  ]
}
