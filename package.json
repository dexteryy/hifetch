{
  "name": "hifetch",
  "version": "2.1.0",
  "description": "A minimal higher-level wrapper around Fetch API",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/hifetch/issues"
  },
  "homepage": "https://github.com/dexteryy/hifetch",
  "repository": "https://github.com/dexteryy/hifetch",
  "license": "MIT",
  "main": "build/dist/index.js",
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "precommit": "npm run lint && npm run test",
    "build": "if [ -d build ]; then rm -r build; fi; npm run build:src && npm run build:test",
    "build:src": "babel src --out-dir build/dist --source-maps",
    "build:test": "babel tests --out-dir build/tests --source-maps",
    "test": "npm run build && npm run test:nobuild",
    "test:nobuild": "mocha build/tests/**/*.spec.js",
    "lint": "eslint src/**"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "chance": "^1.0.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-react": "^6.10.3",
    "fast-async": "^6.2.2",
    "form-data": "^2.1.2",
    "husky": "^0.13.3",
    "mocha": "^3.2.0",
    "nock": "^9.0.11"
  },
  "keywords": [
    "fetch",
    "xhr",
    "ajax",
    "promise",
    "error-handling",
    "isomorphic"
  ]
}
